<odoo>
    <data noupdate="1">
        <record id="account_advance_received_liability" model="account.account">
            <field name="code">234000</field> 
            <field name="name">Advance Received</field>
            <field name="account_type">liability_current</field>
            <field name="reconcile" eval="True"/> 
        </record>

        <record id="journal_advance_payment" model="account.journal">
            <field name="name">Advance Payment Journal</field>
            <field name="code">ADVJ</field>
            <field name="type">general</field>
            <field name="show_on_dashboard" eval="True"/>
        </record>
    </data>
</odoo>